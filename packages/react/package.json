{
  "name": "@shiperist-catppuccin-ui/react",
  "version": "0.0.4",
  "description": "Soothing pastel React components! A Tailwind CSS component library based on Catppuccin.",
  "license": "MIT",
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
   "dev": "tsup src/index.ts --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@shiperist-catppuccin-ui/alert": "*",
    "@shiperist-catppuccin-ui/avatar": "*",
    "@shiperist-catppuccin-ui/badge": "*",
    "@shiperist-catppuccin-ui/button": "*",
    "@shiperist-catppuccin-ui/card": "*",
    "@shiperist-catppuccin-ui/checkbox": "*",
    "@shiperist-catppuccin-ui/divider": "*",
    "@shiperist-catppuccin-ui/icon": "*",
    "@shiperist-catppuccin-ui/indicator": "*",
    "@shiperist-catppuccin-ui/layout": "*",
    "@shiperist-catppuccin-ui/progress": "*",
    "@shiperist-catppuccin-ui/radio": "*",
    "@shiperist-catppuccin-ui/range": "*",
    "@shiperist-catppuccin-ui/select": "*",
    "@shiperist-catppuccin-ui/switch": "*",
    "@shiperist-catppuccin-ui/text-input": "*",
    "@shiperist-catppuccin-ui/typography": "*",
    "@shiperist-catppuccin-ui/textarea": "*",
    "@shiperist-catppuccin-ui/tooltip": "*",
    "@shiperist-catppuccin-ui/menu": "*"
  },
  "devDependencies": {
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "postcss": "^8.4.20",
    "cssnano": "^6.0.1",
    "tailwindcss": "^3.2.4",
    "tailwind-config": "*",
    "tsconfig": "*",
    "tsup": "^6.1.3",
    "autoprefixer": "^10.4.14",
    "@swc/core": "^1.3.73"
  }
}
